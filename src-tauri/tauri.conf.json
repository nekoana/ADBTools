{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "ADBTools",
  "version": "0.0.0",
  "identifier": "com.kouqurong.adbtools",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "adbtools",
        "width": 1080,
        "height": 600,
        "minWidth": 1080,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": ["https://nekoana.github.io/ADBTools/updater.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDgxOTZEQzk2QjE5MTQxQTYKUldTbVFaR3hsdHlXZ2VkUDJZa09jT1pxM2VxcWdYaXRHRDZUekRQN1grRjNwVmxZS2Jja1dIbHIK",
      "windows": {
        "installMode": "quiet"
      }
    }
  }
}
